          <TableColumn fx:id="nameColumn" prefWidth="104.0" text="Name" />
            <TableColumn fx:id="timeColumn" prefWidth="107.0" text="Time" />
            <TableColumn fx:id="locationColumn" prefWidth="79.0" text="Location" />
            <TableColumn fx:id="notesColumn" prefWidth="127.0" text="Notes" />
        </columns>
      </TableView>
      <Button layoutX="125.0" layoutY="568.0" mnemonicParsing="false" onAction="#editUser" prefHeight="26.0" prefWidth="86.0" text="Edit User" />
      <Button layoutX="744.0" layoutY="34.0" mnemonicParsing="false" onAction="#logOut" text="Log Out" />

    </children>
</AnchorPane>
