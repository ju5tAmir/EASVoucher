<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dk.easvoucher.gui.dashboard.admin.AdminController">
    <children>
        <Label layoutX="19.0" layoutY="14.0" text="Admin Panel">
            <font>
                <Font size="27.0" />
            </font>
        </Label>
        <Button layoutX="32.0" layoutY="312.0" mnemonicParsing="false" onAction="#createUser" text="Create User" />
        <Button layoutX="390.0" layoutY="312.0" mnemonicParsing="false" onAction="#deleteEvent" text="Delete Event" />
        <Button layoutX="161.0" layoutY="312.0" mnemonicParsing="false" onAction="#deleteUser" text="Delete User" />
        <TableView fx:id="userTableview" layoutX="274.0" layoutY="73.0" prefHeight="200.0" prefWidth="232.0">
            <columns>
                <TableColumn fx:id="idColumn" prefWidth="75.0" text="Id" />
                <TableColumn fx:id="usernameColumn" prefWidth="75.0" text="Username" />
                <TableColumn fx:id="roleColumn" prefWidth="75.0" text="Role" />
            </columns>
        </TableView>
      <Label fx:id="usernameLabel" layoutX="17.0" layoutY="73.0" />

    </children>
</AnchorPane>
