<AnchorPane prefHeight="625.0" prefWidth="891.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dk.easvoucher.gui.dashboard.admin.AdminController">
    <children>
        <Label layoutX="19.0" layoutY="14.0" text="Admin Panel">
            <font>
                <Font size="27.0" />
            </font>
        </Label>
        <Button layoutX="25.0" layoutY="568.0" mnemonicParsing="false" onAction="#createUser" text="Create User" />
        <Button layoutX="425.0" layoutY="568.0" mnemonicParsing="false" onAction="#deleteEvent" text="Delete Event" />
        <Button layoutX="228.0" layoutY="568.0" mnemonicParsing="false" onAction="#deleteUser" text="Delete User" />
        <TableView fx:id="userTableview" layoutX="25.0" layoutY="167.0" prefHeight="367.0" prefWidth="286.0">
            <columns>
                <TableColumn fx:id="usernameColumn" prefWidth="168.0" text="Username" />
            <TableColumn fx:id="roleColumn" prefWidth="113.0" text="Role" />
            </columns>
        </TableView>
      <Label fx:id="usernameLabel" layoutX="17.0" layoutY="73.0" />
